{
  "compilerOptions": {
    "composite": false,
    "target": "ES2022",
    "useDefineForClassFields": true,
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "types": ["vite/client", "react", "react-dom"],
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "moduleDetection": "force",
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "rootDir": "../../",
    "baseUrl": ".",
    "rootDirs": ["src", "../../smagenda/src"],
    "resolvePackageJsonExports": false,
    "resolvePackageJsonImports": false,
    "typeRoots": ["../../smagenda/node_modules", "../../smagenda/node_modules/@types", "./node_modules/@types"],
    "paths": {
      "react": ["../../smagenda/node_modules/@types/react/index.d.ts", "../../smagenda/node_modules/react/index.js"],
      "react/jsx-runtime": [
        "../../smagenda/node_modules/@types/react/jsx-runtime.d.ts",
        "../../smagenda/node_modules/react/jsx-runtime.js"
      ],
      "react/jsx-dev-runtime": [
        "../../smagenda/node_modules/@types/react/jsx-dev-runtime.d.ts",
        "../../smagenda/node_modules/react/jsx-dev-runtime.js"
      ],
      "react-dom": ["../../smagenda/node_modules/@types/react-dom/index.d.ts", "../../smagenda/node_modules/react-dom/index.js"],
      "react-dom/client": ["../../smagenda/node_modules/react-dom/client"],
      "vite/client": ["../../smagenda/node_modules/vite/client.d.ts"],
      "components/*": ["../../smagenda/src/components/*"],
      "lib/*": ["../../smagenda/src/lib/*"],
      "state/*": ["../../smagenda/src/state/*"]
    }
  },
  "include": ["src", "../../smagenda/src"]
}
